<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="entity" >
	<class name="ProvideMaterialPeriod" table="providematerial_period" >
		<id name="Provider_id" column="Provider_id" type="java.lang.Integer" >
			<generator class="native"/>
		</id>
		<property name="Materials_id" column="Materials_id" type="java.lang.Integer" />		
		<property name="ProvideMaterial_period" column="ProvideMaterial_period" type="java.lang.Integer" />
		<property name="ProvideMaterial_cost" column="ProvideMaterial_cost" type="big_decimal" />
	</class>
</hibernate-mapping>