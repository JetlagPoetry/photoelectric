<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="entity" >
	<class name="Provider" table="provider" >
		<id name="Provider_id" column="Provider_id" type="java.lang.Integer" >
			<generator class="native"/>
		</id>
		<property name="Provider_name" column="Provider_name" type="java.lang.String" />		
		<property name="Provider_address" column="Provider_address" type="java.lang.String" />
		<property name="Provider_contact" column="Provider_contact" type="java.lang.String" />
		<property name="Provider_period" column="Provider_period" type="java.lang.Integer" />
	</class>
</hibernate-mapping>