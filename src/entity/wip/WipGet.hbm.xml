<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="entity.wip" >
	<class name="WipGet" table="wip_get" >
		<id name="Wip_id" column="Wip_id" type="java.lang.Integer" >
			<generator class="native"/>
		</id>
		<property name="Station_id" column="Station_id" type="java.lang.Integer" />		
		<property name="StationGetWip_time" column="StationGetWip_time" type="timestamp" />
		<property name="StationGetWip_amount" column="StationGetWip _amount" type="java.lang.Integer" />
		<property name="StationGetWip_location" column="StationGetWip _location" type="java.lang.Integer" />
		<property name="StationGetWip_status" column="StationGetWip_status" type="java.lang.Integer" />
	</class>
</hibernate-mapping>