<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="entity" >
	<class name="ProductSell" table="productsell" >
		<id name="Ots_id" column="Ots_id" type="java.lang.Integer" >
			<generator class="native"/>
		</id>
		<property name="Order_id" column="Order_id" type="java.lang.Integer" />		
		<property name="ProductSell_amount" column="ProductSell_amount" type="java.lang.Integer" />
	</class>
</hibernate-mapping>