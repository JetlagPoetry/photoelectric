<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="entity.materials" >
	<class name="MaterialsWip" table="materials_wip" >
		<id name="Materials_id" column="Materials_id" type="java.lang.Integer" >
			<generator class="native"/>
		</id>
		<property name="Wip_id" column="Wip_id" type="java.lang.Integer" />		
		<property name="Material_num" column="Material_num" type="java.lang.Integer" />
	</class>
</hibernate-mapping>