<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="entity" >
	<class name="MaterialsStore" table="materialsstore" >
		<id name="MaterialsStore_RepositoryID" column="MaterialsStore_ RepositoryID" type="java.lang.Integer" >
			<generator class="native"/>
		</id>
		<property name="MaterialsStore_MaterialsID" column="MaterialsStore _ MaterialsID" type="java.lang.Integer" />		
		<property name="MaterialsStore_MaterialsNum" column="MaterialsStore_ MaterialsNum" type="java.lang.Integer" />
	
	</class>
</hibernate-mapping>