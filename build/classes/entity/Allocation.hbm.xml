<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="entity" >
	<class name="Allocation" table="allocation" >
		<id name="Allocation_id" column="Allocation_id" type="java.lang.Integer" >
			<generator class="native"/>
		</id>
		<property name="Repository_id" column="Repository_id" type="java.lang.Integer" />		
		<property name="Allocation_stockNo" column="Allocation_stockNo" type="java.lang.Integer" />
		<property name="Allocation_isOccupied" column="Allocation_isOccupied" type="java.lang.Integer"/>
	</class>
	
</hibernate-mapping>