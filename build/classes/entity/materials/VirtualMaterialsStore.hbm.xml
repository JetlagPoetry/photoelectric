<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="entity.materials" >
	<class name="VirtualMaterialsStore" table="virtualmaterialsstore" >
		<id name="VirtualMaterialsStore_RepositoryID" column="VirtualMaterialsStore_ RepositoryID" type="java.lang.Integer" >
			<generator class="native"/>
		</id>
		<property name="VirtualMaterialsStore_MaterialsID" column="VirtualMaterialsStore_ MaterialsID" type="java.lang.Integer" />		
		<property name="VirtualMaterialsStore_OrderID" column="VirtualMaterialsStore _OrderID" type="java.lang.Integer" />
		<property name="VirtualMaterialsStore_MaterialsNum" column="VirtualMaterialsStore_ MaterialsNum" type="java.lang.Integer" />
	</class>
</hibernate-mapping>