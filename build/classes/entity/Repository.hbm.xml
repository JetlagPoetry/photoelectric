<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="entity" >
	<class name="Repository" table="repository" >
		<id name="Repository_id" column="Repository_id" type="java.lang.Integer" >
			<generator class="native"/>
		</id>
		<property name="Repository_address" column="Repository_address" type="java.lang.String" />		
		<property name="Repository_manageID" column="Repository_manageID" type="java.lang.Integer" />
	</class>
</hibernate-mapping>