<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="entity.wip" >
	<class name="WipOts" table="wip_ots" >
		<id name="Wip_id" column="Wip_id" type="java.lang.Integer" >
			<generator class="native"/>
		</id>
		<property name="Ots_id" column="Ots_id" type="java.lang.Integer" />		
		<property name="Wip_num" column="Wip_num" type="java.lang.Integer" />
	</class>
</hibernate-mapping>