<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="entity.wip" >
	<class name="WipStockManage" table="wip_stockmanage" >
		<id name="Wip_id" column="Wip_id" type="java.lang.Integer" >
			<generator class="native"/>
		</id>
		<property name="Repository_id" column="Repository_id" type="java.lang.Integer" />		
		<property name="Allocation_id" column="Allocation_id" type="java.lang.Integer" />
		<property name="Wip_amount" column="Wip_amount" type="java.lang.Integer" />
		<property name="Wip_StockDealPersonID" column="Wip_StockDealPersonID" type="java.lang.Integer" />
		<property name="Wip_takePersonID" column="Wip_takePersonID" type="java.lang.Integer" />
		<property name="WipStockManage_type" column="WipStockManage_type" type="java.lang.Integer" />
		<property name="Wip_bringPersonID" column="Wip_bringPersonID" type="java.lang.Integer" />
		<property name="WipStockManage_ID" column="WipStockManage_ID" type="java.lang.String" />
		<property name="WipStockManage_Date" column="WipStockManage_Date" type="timestamp" />
	</class>
</hibernate-mapping>